-- Script para obtener hist贸rico de empleados
-- creator: yosmasan
Select * 
from empleado
where emp_tipoliq = "M"
and emp_estado = "N"
and emp_fechIngreso > to_date("01/01/2000","DD/MM/YYYY");


-- Script para obtener hist贸ricos de pensionados
--  creator: yamileosma
Select * 
from empleado
where emp_tipoliq = "Q"
and emp_estado = "N" 

